{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Your Documentation",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
 "colors": {
  "primary": "#0D9373",
  "light": "#07C983",
  "dark": "#0D9373"
},
  "theme": "mint",
  "topbar": {
    "style": "default",
    "cta": {
      "name": "Get Started",
      "url": "https://yourapp.com/signup"
    }
  },
"navigation": {
  "dropdowns": [
    {
      "dropdown": "Documentation",
      "icon": "book-open",
      "pages": [
        "quickstart",
        "development",
        "navigation"
      ]
    },
    {
      "dropdown": "API References",
      "icon": "square-terminal",
      "pages": [
        "api-reference/get",
        "api-reference/post",
        "api-reference/delete"
      ]
    },
    {
      "dropdown": "Blog",
      "href": "https://external-link.com/blog"
    }
  ]
},
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["curl", "python", "javascript", "go"],
      "defaultLanguage": "python"
    },
    "mdx": {
      "server": "https://api.yourapp.com",
      "auth": {
        "method": "bearer"
      }
    }
  },
  "seo": {
    "indexing": "all",
    "metatags": {
      "og:type": "website",
      "og:site_name": "Your Documentation",
      "twitter:card": "summary_large_image"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/yourhandle",
      "github": "https://github.com/yourorg",
      "discord": "https://discord.gg/yourserver"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  }
}
