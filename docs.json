{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Your Documentation",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
 "colors": {
  "primary": "#0D9373",
  "light": "#07C983",
  "dark": "#0D9373"
},
  "theme": "mint",
  "topbar": {
    "style": "default",
    "cta": {
      "name": "Get Started",
      "url": "https://yourapp.com/signup"
    }
  },
  "navigation": {
    "anchors": [
      {
        "anchor": "Documentation",
        "icon": "book-open",
        "pages": [
          "quickstart",
          "development",
          "navigation"
        ]
      },
      {
        "anchor": "Community",
        "icon": "discord",
        "href": "https://discord.gg/yourserver"
      }
    ],
    "tabs": [
      {
        "tab": "Getting Started",
        "icon": "rocket",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "index",
              "quickstart",
              "installation"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "settings",
              "themes",
              "navigation"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "code",
        "openapi": "/openapi.json"
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["curl", "python", "javascript", "go"],
      "defaultLanguage": "python"
    },
    "mdx": {
      "server": "https://api.yourapp.com",
      "auth": {
        "method": "bearer"
      }
    }
  },
  "seo": {
    "indexing": "all",
    "metatags": {
      "og:type": "website",
      "og:site_name": "Your Documentation",
      "twitter:card": "summary_large_image"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/yourhandle",
      "github": "https://github.com/yourorg",
      "discord": "https://discord.gg/yourserver"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  }
}
